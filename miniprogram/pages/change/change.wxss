.white-container { 
  margin-top: 550rpx;
  height: 500rpx;
  width: 100%;
    display: flex;  
    flex-direction: column;  
    justify-content: center; /* 垂直居中 */  
    align-items: center; /* 水平居中（对于单个项目）*/  
    flex: 1; /* 让白色背景容器占据剩余空间 */  
  
    
  }  
    
  .input-group {  
    display: flex;  
    flex-direction: column;  
    align-items: center; /* 使得内部的输入框在水平方向上居中（如果需要）*/  
    margin-top: 0rpx;
    width: 100%; /* 设置宽度为容器宽度 */  
  }  
    
  .input-box {  
    left: 0rpx;
    top: 0rpx;
    width: 600rpx;
    height: 100rpx;
    opacity: 1;
    border-radius: 48rpx;
    background: rgba(255, 255, 255, 1);
    border: 1px solid rgba(204, 214, 203, 1);
    display: flex;
    justify-content: flex-start;
    align-items: center;
    padding: 0rpx 30rpx 0rpx 30rpx;
    margin: 30rpx;
  }  
    
  .submit-button {  
  
  width: 270rpx;
  height: 80rpx;
  
  border-radius: 48rpx;
  background: rgba(204, 204, 204, 0);
