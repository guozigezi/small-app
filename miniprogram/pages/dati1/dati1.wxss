  /* miniprogram/pages/testSAS/testSAS.wxss */

 
.sheet {
  background: #fff;
  border-radius: 36rpx;
  margin-top: 400rpx;
  margin-left: 55rpx;
  width: 84%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 20rpx;
}
.naire {
  width: 100%;
}
.question {
  font-size: 40rpx;
  font-family:"microsoft yahei";
  font-weight: bold;
  
}
.border {
  height: 2rpx;
  width: 90%;
  background-color: #d4d4d4;
  margin: 16rpx auto;
}
.radio-group {
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  text-indent: 14rpx;
  margin-top: 40rpx;
}
.radio {
  margin-bottom: 30rpx;
  font-family: 宋体;
  margin-top:10rpx ;
}
.radio1{
  display: inline-block;
  margin-top: 20rpx;
}
.button-group {
  display: flex;
  flex-direction: row;
  width: 75%;
  margin: auto;
  justify-content: space-between;
  margin-top: 10rpx;
}
.button {
  background: #002fb1;
  border-radius: 100rpx;
  font-size: 30rpx;
  line-height: 52rpx;
  height: 85rpx;
  width: 100rpx;
  font-weight: bold;
  color: #fff;
  margin: 30rpx;
}
.button::after {
  border: 0;
}
.id {
  text-align: center;
  font-size: 24rpx;
  color: #c2c2c2;
  margin: auto;
  margin-top: 20rpx;
}
.song{
  font-family: 宋体;
  font-size: 35rpx;
}